    <warnings>
      <!-- type: periodEnd|lectureEnd|validTo -->
      <warning type="periodEnd" at="11.01." template="e-mail-semapp-inactivate" groupTemplate="e-mail-semapp-warning-group" />
      <warning type="periodEnd" at="01.03." template="e-mail-semapp-inactivate" groupTemplate="e-mail-semapp-warning-group" />
